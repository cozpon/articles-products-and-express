<h1> Products, daddio! </h1>
<a href="/products/new" class="button">Make New Product!</a>
  <div class="products">
  <ul class='productList'>
    {{#each productsList}}
      <br>
      <li>ID : {{id}}</li>
      <li>Name : {{name}}</li>
      <li>Price : {{price}}</li>
      <li>Inventory : {{inventory}}</li>
      <br>
      <form action="/products/{{id}}/edit" method="GET" >
       <input type="submit" value="Edit Product">
      </form>

    {{/each}}
  </ul>
</div>

{{> footer}}
