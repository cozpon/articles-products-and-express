<h1> Articles yo! </h1>
<a href="/articles/new" class="button">Write new Article!</a>
<div class="articles">
  <ul class='articleList'>
    {{#each articlesList}}
      <br>
      <li>title : {{title}}</li>
      <li>author : {{author}}</li>
      <li>body : {{body}}</li>
      <br>
      <form action="/articles/{{title}}/edit" method="GET" >
       <input type="submit" value="Edit Product">
      </form>

    {{/each}}


{{> footer}}